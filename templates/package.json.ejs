{
  "name": "<%- appName %>",
  "version": "<%- appVersion %>",
  "description": "<%- appDescription %>",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test multi='xunit=test-results/xunit.xml spec=-' node_modules/istanbul-harmony/lib/cli.js cover node_modules/mocha/bin/_mocha -- --ui bdd -R mocha-multi -t 5000",
    "test-w": "NODE_ENV=test node_modules/mocha/bin/mocha -w",
